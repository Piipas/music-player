{
    "compilerOptions": {
        "lib": ["ESNext"],
        "module": "esnext",
        "outDir": "./dist",
        "target": "esnext",
        "moduleResolution": "bundler",
        "moduleDetection": "force",
        "strict": true,
        "downlevelIteration": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true
    }
}
